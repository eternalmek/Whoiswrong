{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/health",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*\\.(js|css|html|png|jpg|jpeg|gif|ico|svg|woff|woff2|webp|json|txt|xml))",
      "dest": "/public/$1"
    },
    {
      "src": "/account.html",
      "dest": "/public/account.html"
    },
    {
      "src": "/account",
      "dest": "/public/account.html"
    },
    {
      "src": "/account/",
      "dest": "/public/account.html"
    },
    {
      "src": "/checkout/success",
      "dest": "/public/checkout/success/index.html"
    },
    {
      "src": "/success",
      "dest": "/public/success/index.html"
    },
    {
      "src": "/checkout/cancel",
      "dest": "/public/checkout/cancel/index.html"
    },
    {
      "src": "/cancel",
      "dest": "/public/cancel/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    }
  ]
}
